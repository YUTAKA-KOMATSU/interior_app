<div class="maps">
  <div id='map'></div>
  <style>
  #map {
    height: 600px;
    width: 1000px;
    
  }
  </style>

  <script>
  let map

  function initMap(){
    geocoder = new google.maps.Geocoder()

    map = new google.maps.Map(document.getElementById('map'), {
      center: {lat:35.658446353639725, lng:139.7388799722884},
      zoom: 12,
    });

    marker = new google.maps.Marker({
      position:  {lat:35.658446353639725, lng:139.7388799722884},
      map: map
    });
  }
  </script>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC9Ns6Ao8wS7NZDQe137LHlm24zaFGtcsA&callback=initMap" async defer></script>
</div>
